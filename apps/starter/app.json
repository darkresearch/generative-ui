{
  "expo": {
    "name": "Darkling",
    "slug": "starter",
    "scheme": "starter",
    "version": "0.1.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "icon": "./assets/app-icon.png",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.darkresearch.starter",
      "icon": "./assets/app-icon.png"
    },
    "android": {
      "package": "com.darkresearch.starter",
      "icon": "./assets/app-icon.png"
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-asset",
      "expo-font"
    ],
    "experiments": {
      "autolinkingModuleResolution": true
    }
  }
}
